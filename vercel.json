{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/style.css",
            "destination": "/style.css"
        },
        {
            "source": "/script.js",
            "destination": "/script.js"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ]
}